{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "name": "Quiz",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "answerId": {
      "type": "integer",
      "required": true
    },
    "Option": {
      "type": "component",
      "repeatable": true,
      "component": "option.option"
    },
    "CodeSnippet": {
      "type": "component",
      "repeatable": false,
      "component": "code.code-snippet"
    },
    "issue": {
      "via": "quiz",
      "model": "issues"
    }
  }
}
